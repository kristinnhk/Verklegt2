@model Stoker.Models.ViewModel
<nav class="navbar-left col-md-2" id="sidebarEveryPage">
	<!--<link href="~/Content/SidebarStyleSheet.css" rel="stylesheet" />-->
	<link href="~/Content/FrameStyleSheet.css" rel="stylesheet" />
	<div class="interestsSidebar">
		<h3>Interests</h3>
		<ul>
			@for (int i = 1; i <= 20; i++)
			{
				<li>interest @i</li>
			}
			@*foreach (var m in Model.interests)
				{
						<li>@Html.ActionLink(@m.name, "InterestViewController", @m.interestID)</li>
				}*@
		</ul>
	</div>
	<div class="groupsSidebar">
		<h3>Groups</h3>
		<ul>
			@for (int i = 1; i <= 300; i++)
			{
				<li>group @i</li>
			}
			@*foreach(var m in Model.groups)
				{
						<li><a>@m.title</a></li>
				}*@
		</ul>
	</div>
</nav>