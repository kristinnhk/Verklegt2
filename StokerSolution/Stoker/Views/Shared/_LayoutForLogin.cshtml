<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - My ASP.NET Application</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
	@*<link rel="stylesheet" type="text/css" href="~/Content/FrameStyleSheet.css">*@
	@*<link rel="stylesheet" type="text/css" href="~/Content/ThreadStyleSheet.css">
	<link rel="stylesheet" type="text/css" href="~/Content/pageMainContent.css">*@
</head>
<body>
	<div class="body-content">
		<div class="container clearfix">
			@RenderBody()
		</div>
	</div>
	@*@Html.Partial("FooterPartialView")*@
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@RenderSection("scripts", required: false)
</body>
</html>
